{
  "name": "vcwebnetworks-nodets",
  "version": "1.0.0",
  "main": "dist/server.js",
  "repository": "git@github.com:vagnercardosoweb/vcwebnetworks-nodets.git",
  "author": "Vagner Cardoso <vagnercardosoweb@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=12.18.3"
  },
  "scripts": {
    "dev": "ts-node-dev --poll --respawn --transpile-only --ignore-watch node_modules './src/server.ts'",
    "start": "yarn build && node ./dist/server.js",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js",
    "lint": "eslint './src/**' --ext ts",
    "lint:fix": "yarn lint --fix",
    "docker": "docker-compose -f docker-compose.yml up -d"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "yarn lint:fix"
    ]
  },
  "dependencies": {
    "celebrate": "13.0.2",
    "debug": "4.2.0",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-async-errors": "3.1.1",
    "helmet": "4.1.1",
    "module-alias": "2.2.2",
    "mysql2": "2.1.0",
    "reflect-metadata": "0.1.13",
    "typeorm": "0.2.26"
  },
  "devDependencies": {
    "@babel/cli": "7.11.6",
    "@babel/core": "7.11.6",
    "@babel/node": "7.10.5",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/plugin-proposal-decorators": "7.10.5",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-typescript": "7.10.4",
    "@types/cors": "2.8.7",
    "@types/debug": "4.1.5",
    "@types/express": "4.17.8",
    "@types/helmet": "0.0.48",
    "@types/module-alias": "2.0.0",
    "@types/node": "14.10.1",
    "@typescript-eslint/eslint-plugin": "4.1.0",
    "@typescript-eslint/parser": "4.1.0",
    "babel-plugin-transform-typescript-metadata": "0.3.0",
    "eslint": "7.9.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-import-resolver-typescript": "2.3.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-import-helpers": "1.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "git-commit-msg-linter": "2.6.0",
    "husky": "4.3.0",
    "lint-staged": "10.3.0",
    "prettier": "2.1.1",
    "ts-node-dev": "1.0.0-pre.62",
    "typescript": "3.9.7"
  }
}
