{
  "name": "vcwebnetworks-nodets",
  "version": "1.0.0",
  "main": "./dist/server.js",
  "repository": "git@github.com:vagnercardosoweb/vcwebnetworks-nodets.git",
  "author": "Vagner Cardoso <vagnercardosoweb@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">= 14"
  },
  "scripts": {
    "dev": "ts-node-dev --poll --respawn --transpile-only --ignore-watch node_modules './src/server.ts'",
    "start": "yarn build && node ./dist/server.js",
    "build": "babel ./src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js",
    "lint": "eslint --ext ts ./src/**",
    "lint:fix": "yarn lint --fix",
    "docker": "docker-compose -f docker-compose.yml up"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "yarn lint:fix"
    ]
  },
  "dependencies": {
    "@sentry/node": "^5.29.2",
    "bcryptjs": "^2.4.3",
    "celebrate": "^13.0.4",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^4.3.1",
    "ioredis": "~4.19.4",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.29",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.12",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.9",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.9",
    "@types/helmet": "^4.0.0",
    "@types/ioredis": "^4.17.10",
    "@types/module-alias": "^2.0.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.19",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "babel-plugin-transform-typescript-metadata": "^0.3.1",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "git-commit-msg-linter": "^3.0.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  }
}
